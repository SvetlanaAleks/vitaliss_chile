section.catalog-main
	.container
		.catalog-main__content
			.catalog-main__left
				p.category-item#beauty.js-tab.active Для КРАСОТЫ
				p.category-item#health.js-tab Для ЗДОРОВЬЯ
			.catalog-main__right
				.catalog-main__list.js-list-prod.active(data-target="beauty")
					.catalog-main__wrap.catalog-main__wrap--small.catalog-main__wrap--show.js-catalog-show
						each product in productsBeauty
							+product(product)
					p.catalog-main__count.js-catalog-count
						span.js-product-show 0
						|  из  
						span.js-product-all 0
					button.catalog-main__btn.btn.js-btn-catalog Показать больше
				.catalog-main__list.js-list-prod(data-target="health")
					.catalog-main__wrap.catalog-main__wrap--show.js-catalog-show
						each product in productsHealth
							+product(product)
					p.catalog-main__count.js-catalog-count
						span.js-product-show 0
						|  из  
						span.js-product-all 0